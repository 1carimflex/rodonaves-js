  "main": "dist/rodonaves-js.min.js",
  "module": "dist/rodonaves-js-browser.min.js",
  "browser": "dist/rodonaves-js-browser.min.js",
  "scripts": {
    "build-browser": "rollup -c && uglifyjs dist/cep-promise-browser.js -cm -o dist/cep-promise-browser.min.js",
