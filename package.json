    "eslint-plugin-import": "^2.20.2",
    "husky": "^4.2.5",
    "jest": "^25.3.0",
    "rollup": "^2.6.1",
    "rollup-plugin-babel": "^4.4.0",
    "semantic-release": "^17.0.4",
    "uglify-js": "^3.8.1"
  },
  "release": {
    "branches": [
      "master"
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E  HUSKY_GIT_PARAMS"
    }
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
